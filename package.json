{
  "name": "@tetri/semantic-versioning",
  "version": "0.0.0-development",
  "description": "Implementação de versionamento semântico (SemVer 2.0.0) em TypeScript com suporte a parsing, comparação e serialização JSON.",
  "license": "MIT",
  "author": "Tetri Mesquita",
  "repository": {
    "type": "git",
    "url": "https://github.com/tetri/SemanticVersioning.git"
  },
  "homepage": "https://github.com/tetri/SemanticVersioning",
  "bugs": {
    "url": "https://github.com/tetri/SemanticVersioning/issues"
  },
  "keywords": [
    "semver",
    "versioning",
    "semantic-version",
    "semantic-versioning"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node": "^22.13.1",
    "@eslint/js": "^9.18.0",
    "eslint": "^9.18.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0",
    "tsup": "^8.4.0",
    "vitest": "^2.1.8"
  }
}
